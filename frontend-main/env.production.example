# Production Environment Variables Example
# Скопируйте этот файл в .env.production перед деплоем

# API Configuration
NEXT_PUBLIC_API_MAIN_ENDPOINT=https://appserver.faithtribe.io
NEXT_PUBLIC_LIVE=true
NEXT_PUBLIC_LOCAL_DEBUG=false
NEXT_PUBLIC_IS_PROXY=false

# Authentication (заполните реальными значениями перед деплоем)
NEXT_PUBLIC_DEBUG_SHOPIFY_ID=
NEXT_PUBLIC_DEBUG_DESIGNER_ID=
NEXT_PUBLIC_CUSTOMER_LOGIN_REDIRECT_URL=https://configurator.site

# Node Environment
NODE_ENV=production

# API Endpoints
NEXT_PUBLIC_API_MEDIA_ENDPOINT=https://configurator.site:5002/Media/Upload

# Analytics (опционально)
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=
